#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

        int units, bill;
scanf("%d", &units);
if (units <= 200) {
    bill = units * 0.5;
} else if (units <= 400) {
    bill = units * 0.65 + 100;
} else if (units <= 600) {
    bill = units * 0.80 + 200;
} else {
    bill = units * 1.25 + 425;
}
printf("Rs.%d\n", bill);
    return 0;
}
