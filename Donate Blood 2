#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    
      int a, w, h;
      scanf("%d%d%d", &a, &w, &h);
      if(a>=18&&w>=45&&h>=5.5) 
{
      printf("He or She can donate blood");
}
else
{
      printf("He or She unable to donate blood");
}

    return 0;
}
